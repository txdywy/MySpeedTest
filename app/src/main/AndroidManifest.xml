<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.num.myspeedtest" >

    <application
        android:allowBackup="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.ThroughputActivity"
            android:label="@string/title_activity_throughput" >
        </activity>
        <activity
            android:name=".activities.ThroughputHistoryActivity"
            android:label="@string/title_activity_throughput_history" >
        </activity>
        <activity
            android:name=".activities.LatencyActivity"
            android:label="@string/title_activity_latency" >
        </activity>
        <activity
            android:name=".activities.DataUsageActivity"
            android:label="@string/title_activity_data_usage" >
        </activity>
    </application>

</manifest>
